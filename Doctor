class Doctor {
    void consultationFee() {
        System.out.println("General consultation fee");
    }
}

class Dentist extends Doctor {
    void consultationFee() {
        System.out.println("Dentist Fee: ₹500");
    }
}

class Cardiologist extends Doctor {
    void consultationFee() {
        System.out.println("Cardiologist Fee: ₹1500");
    }
}

class Surgeon extends Doctor {
    void consultationFee() {
        System.out.println("Surgeon Fee: ₹2500");
    }
}

public class Main {
    public static void main(String[] args) {
        Doctor d1 = new Dentist();
        Doctor d2 = new Cardiologist();
        Doctor d3 = new Surgeon();

        d1.consultationFee();
        d2.consultationFee();
        d3.consultationFee();
    }
}
